{
  "name": "ttytube",
  "description": "CLI viewer for youtube",
  "version": "0.0.0",
  "author": "Geoffroy Warin @geowarin",
  "bin": {
    "ttytube": "./bin/run"
  },
  "bugs": "https://github.com/geowarin/ttytube/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.1",
    "cheerio": "^1.0.0-rc.5",
    "clean-text-utils": "^1.1.17",
    "execa": "^5.0.0",
    "got": "^11.8.1",
    "import-jsx": "^4.0.0",
    "ink": "^3.0.8",
    "react": "^17.0.1",
    "react-query": "^3.6.0",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.51",
    "@types/react": "^17.0.0",
    "chai": "^4.2.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/geowarin/ttytube",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ttytube",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "geowarin/ttytube",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test:cov": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
